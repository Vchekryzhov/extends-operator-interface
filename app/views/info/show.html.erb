
<div class="main-menu">
  <div>
    <h5 class="center-align"><%= @machine.title %></h5>
  </div>
  <ul class="collapsible" id="real-time-data">
    <li v-for="section in data">
      <div class="collapsible-header">
        <i class="material-icons">add</i> {{section[0]}}
      </div>
      <div class="collapsible-body">
        <table class="striped">
          <thead>
            <tr>
              <th><%= I18n.t(:title) %></th>
              <th><%= I18n.t(:value) %></th>
            </tr>
          </thead>
          <tbody>
            <tr v-for="item in section[1]">
              <td>
                {{ item[0] }}
              </td>
              <td>
                {{ item[1] }}
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </li>
  </ul>
</div>
