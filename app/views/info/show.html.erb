
<div class="main-menu">
  <ul class="collapsible">
    <li>
      <div class="collapsible-header">
        <i class="material-icons">build</i><%= I18n.t(:mototime) %></div>
      <div class="collapsible-body">
        <table id="mototime-data" class="striped">
          <thead>
            <tr>
              <th><%= I18n.t(:title) %></th>
              <th><%= I18n.t(:value) %></th>
            </tr>
          </thead>
          <tbody>
            <tr v-for="item in items">
              <td>
                {{ item[0] }}</td>
              <td>
                {{ item[1] }}</td>
            </tr>
          </tbody>
        </table>
      </div>
    </li>
    <li>
      <div class="collapsible-header">
        <i class="material-icons">visibility</i><%= I18n.t(:plc) %></div>
      <div class="collapsible-body">
        <table id="plc-data" class="striped">
          <thead>
            <tr>
              <th><%= I18n.t(:title) %></th>
              <th><%= I18n.t(:value) %></th>
            </tr>
          </thead>
          <tbody>
            <tr v-for="item in items">
              <td>
                {{ item[0] }}</td>
              <td>
                {{ item[1] }}</td>
            </tr>
          </tbody>
        </table>
      </div>
    </li>
  </ul>
</div>
